@model IEnumerable<RegisteredInvoice>

@{
    ViewData["Title"] = "Index";
}
<partial name="_Notification" />

<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-black text-primary">Наявні Іменні Накладні</h2>
        </div>
    </div>
    <br />

    <table class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr class="table-primary">
                <th>Клієнт</th>
                <th>Товар</th>
                <th>Дата прийняття</th>
                <th>Дата закінчення зберігання</th>
        </thead>
        <tbody>
            @foreach (var obj in Model)
            {
                <tr>
                    <td width="34%">@obj.Client.FullName</td>
                    <td width="30%">@obj.Good.Name</td>
                    <td width="23%">@obj.ReceiptDate</td>
                    <td width="23%">@obj.StorageDate</td>
                </tr>
            }
        </tbody>
    </table>
</div>
